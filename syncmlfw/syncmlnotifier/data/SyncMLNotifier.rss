// -----------------------------------------------------------------------------
//   
// r_newalert_date_query
// Data Query for Date Input type.
//
// -----------------------------------------------------------------------------
//   
RESOURCE DIALOG r_newalert_date_query
    {
    flags = EGeneralQueryFlags;
    buttons = R_AVKON_SOFTKEYS_OK_CANCEL__OK;
    
    items=
        {
        DLG_LINE
            {
            type = EAknCtQuery;
            id = EGeneralQuery;
            control = AVKON_DATA_QUERY 
                { 
                layout = EDateLayout;
                
               	 	control =  DATE_EDITOR
               		{
                		minDate = DATE
										{
										year = 0001;
											};
										maxDate = DATE
										{
										year = 9999;
										};
										flags = 0;
                		
                		
                	};
                };
            }
        };
    } 
    
// -----------------------------------------------------------------------------
//   
// r_newalert_time_query
// Data Query for Date Input type.
//
// -----------------------------------------------------------------------------
//   
    RESOURCE DIALOG r_newalert_time_query
    {
    flags = EGeneralQueryFlags;
    buttons = R_AVKON_SOFTKEYS_OK_CANCEL__OK;
    
    items=
        {
        DLG_LINE
            {
            type = EAknCtQuery;
            id = EGeneralQuery;
            control = AVKON_DATA_QUERY 
                { 
                layout = ETimeLayout;
                
               	 	control =  TIME_EDITOR
               		{
                		
                		minTime = TIME
						{
						second = 0;
						minute = 0;
						hour = 0;
						};
						maxTime = TIME
						{
						second = 59;
						minute = 59;
						hour = 23;
						};
					flags = EEikTimeForce24HourFormat/*|EEikTimeWithoutSecondsField*/; //remove this flag to get seconds
					
                		
                	};
                };
            }
        };
    } 
    
